div.tools-container()
	div.tools.dijitHidden()
		span(data-bind="click:addMap")
			button(data-dojo-type="dijit/form/Button" data-dojo-props="showLabel: true" type="button") Add Map
		span(data-bind="click:removeMap")
			button(data-dojo-type="dijit/form/Button" data-dojo-props="showLabel: true" type="button") Remove Map
		span(data-bind="click:function(){showMaps(4)}")
			button(data-dojo-type="dijit/form/Button" data-dojo-props="showLabel: true" type="button") 4
		span(data-bind="click:function(){showMaps(2)}")
			button(data-dojo-type="dijit/form/Button" data-dojo-props="showLabel: true" type="button") 2
		span(data-bind="click:function(){showMaps(6)}")
			button(data-dojo-type="dijit/form/Button" data-dojo-props="showLabel: true" type="button") 6
	div.help.dijitHidden()
		span(data-bind="click:showHelp")
			button(data-dojo-type="dijit/form/Button" data-dojo-props="showLabel: true" type="button") Help ?
	div.map-tools()
		div(class="share-container dijitHidden")
			div(class="addthis_sharing_toolbox")
		div(class="geocode-container dijitHidden")
			div(class="geocode-holder")
				input(id="locator" type="text" placeHolder="Search address")
