span(data-bind="foreach:themes")
	//- You have zoomed to 
	div.themeItem(data-bind="event:{mouseenter:$parent.themeSelectHandler}")
		div.themeTitle(data-bind="html:name")
		div.mapIndex()
			div(data-bind="click:function(){$parent.selectedMapIndex(1)}") Map 1
			div(data-bind="click:function(){$parent.selectedMapIndex(2)}") Map 2
			div(data-bind="click:function(){$parent.selectedMapIndex(3)}") Map 3

	
