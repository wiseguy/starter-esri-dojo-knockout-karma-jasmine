div.map-container()
	div#map()
		div(class="basemap-gallery-titlepane-holder")
			div(data-dojo-type="dijit/TitlePane" id="basemap-gallery" data-dojo-props="title: 'Basemap', closable:false, open:true")
				div(data-dojo-type="dijit/layout/ContentPane" class="basemap-contentpane")
					div.basemap-gallery-container()
						div.basemap-gallery()
		div(class="legend-titlepane-holder")
			div(data-dojo-type="dijit/TitlePane" id="legend-gallery" data-dojo-props="title: 'Legend', closable:false, open:true")
				div(data-dojo-type="dijit/layout/ContentPane" class="legend-contentpane")
					div.legend-container()
						div.legend()
